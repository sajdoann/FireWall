#compiler
CXX=g++
CXXFLAGS = -Wall -pedantic -g  -std=c++14 #-fsanitize=address

#linker
LD=g++
LDFLAGS = -Wall -pedantic -g  -std=c++14 #-fsanitize=address

#my name of the executable
EXECUTABLE=sajdoann			# count num of lines in files: find . -name '*.cpp' -o -name '*.h' | xargs wc -l

#comples and generates documentation
all: compile doc

#generates documentation
doc:
	doxygen Doxyfile

#makes directory for .o files
objects:
	mkdir objects

# get all .o files: cat tmp.txt | grep -o '^.*\.o' > jmena.txt
compile: objects/App.o objects/Commands.o  objects/Interface.o  objects/main.o objects/Game.o objects/ScoreCounter.o objects/Board.o objects/Reader.o objects/Writer.o objects/Coords.o objects/Empty.o objects/Hotfix.o objects/Patch.o objects/Virus.o objects/BfsStrategy.o objects/FrontStrategy.o objects/RandomStrategy.o objects/Strategy.o
	$(LD) $(LDFLAGS) objects/App.o objects/Commands.o  objects/Interface.o  objects/main.o objects/Game.o objects/ScoreCounter.o objects/Board.o objects/Reader.o objects/Writer.o objects/Coords.o objects/Empty.o objects/Hotfix.o objects/Patch.o objects/Virus.o objects/BfsStrategy.o objects/FrontStrategy.o objects/RandomStrategy.o objects/Strategy.o   -o $(EXECUTABLE)

#command make run
run: compile
	./$(EXECUTABLE)

#cleans - .o files and executable removed
.PHONY: clean all compile #phony - directive says clean is not a filename
clean:
	rm -rf objects $(EXECUTABLE) doc/ 2>/dev/null
	#rm *.o		in case object files were in this file


#writen rules for .o files ( more rules because of the hierarchy )
objects/%.o: src/%.cpp
	$(LD) $(CXXFLAGS) -c $< -o $@	#makes .o
objects/%.o: src/Game/%.cpp
	$(LD) $(CXXFLAGS) -c $< -o $@
objects/%.o: src/Game/Board/%.cpp
	$(LD) $(CXXFLAGS) -c $< -o $@
objects/%.o: src/Game/IOLoaders/%.cpp
	$(LD) $(CXXFLAGS) -c $< -o $@
objects/%.o: src/Game/Movement/%.cpp
	$(LD) $(CXXFLAGS) -c $< -o $@
objects/%.o: src/Game/Objects/%.cpp
	$(LD) $(CXXFLAGS) -c $< -o $@
objects/%.o: src/Game/Strategics/%.cpp
	$(LD) $(CXXFLAGS) -c $< -o $@

#generated by g++ -MM src/*.cpp src/*/*.cpp src/*/*/*.cpp > tmp.txt for easier reading)
# \.h$   objects\n
